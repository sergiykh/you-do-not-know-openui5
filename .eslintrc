{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "rules": {
    "strict": 0,
    "no-alert": 0,
    "no-bitwise": 0,
    "camelcase": 1,
    "curly": 1,
    "eqeqeq": 0,
    "no-eq-null": 0,
    "guard-for-in": 1,
    "no-empty": 1
  },
  "globals": {
    "$": true,
    "_": true,
    "jQuery": true,
    "sap": true,
    "app": true
  },
  "env": {
    "browser": true,
    "node": true
  }
}
